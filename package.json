{
  "name": "coruja-labs",
  "version": "1.0.0",
  "description": "Projeto que contempla o desafio de engenheiro de qualidade da coruja labs",
  "main": "index.js",
  "scripts": {
    "ui": "./node_modules/.bin/cypress open --config testFiles=**/*.feature",
    "e2e": "./node_modules/.bin/cypress run --config testFiles=**/*.feature",
    "api": "./node_modules/.bin/cypress run"
  },
  "cypress-cucumber-preprocessor": {
    "step_definitions": "cypress/tests"
  },
  "keywords": [
    "test",
    "automation",
    "api",
    "e2e",
    "quality"
  ],
  "author": "Pedro Martins",
  "license": "ISC",
  "devDependencies": {
    "@bahmutov/cy-api": "^1.5.0",
    "cucumber": "^7.0.0-rc.0",
    "cypress": "^7.6.0",
    "cypress-cucumber-preprocessor": "^4.1.3",
    "faker": "^5.5.3"
  }
}
